            "CertificateStoreLocation": "LocalMachine",
            "CertificateStoreName": "My",
            "VisibilityTimeout": "00:05:00",
            "MaxSendRetryCount": 5
        },
        "EnableWebApi": true,
        "WebApi": {
            "BaseAddress": "https://SRVCM01.MSINTUNE.LAB:5000",
            "IngestionRoute": "/api/SecureBootReports",
